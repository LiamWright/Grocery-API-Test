  "dependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.7",
    "@types/mongoose": "^5.7.36",
    "@types/node": "^14.10.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^5.0.0-alpha.7",
