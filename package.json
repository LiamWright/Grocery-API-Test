    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "mongoose": "^5.10.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "tsc": "^1.20150623.0",
    "typescript": "^4.0.2"
  }
