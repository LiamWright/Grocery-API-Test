    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__test__/*.+(ts|tsx|js)"
    ]
  },
  "repository": {
    "type": "git",
